{
  "name": "@joshdb/redis",
  "version": "1.0.0",
  "description": "A provider for @joshdb/core which uses MongoDB",
  "author": "@joshdb",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "browser": "dist/index.umd.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "sideEffects": false,
  "scripts": {
    "test": "vitest run",
    "docs": "typedoc-json-parser",
    "lint": "eslint src tests --ext ts --fix -c ../../.eslintrc",
    "build": "tsup",
    "prepack": "yarn build",
    "bump": "cliff-jumper",
    "check-update": "cliff-jumper --dry-run"
  },
  "dependencies": {
    "@joshdb/provider": "2.0.0-next.cd2b620.0",
    "better-serialize": "^2.0.0",
    "redis": "^4.7.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@favware/cliff-jumper": "^3.0.3",
    "@types/uuid": "^9.0.8",
    "@vitest/coverage-v8": "^1.6.0",
    "tests": "workspace:*",
    "tsup": "^8.2.4",
    "typedoc": "^0.26.5",
    "typedoc-json-parser": "^10.1.5",
    "typescript": "^5.5.4",
    "vitest": "^1.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/josh-development/providers.git"
  },
  "files": [
    "dist",
    "!dist/*.tsbuildinfo"
  ],
  "engines": {
    "node": ">=20",
    "npm": ">=7"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/josh-development/providers/issues"
  },
  "homepage": "https://josh.evie.dev",
  "publishConfig": {
    "access": "public"
  }
}
